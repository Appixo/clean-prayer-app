{
  "expo": {
    "name": "Namaz Vakitleri",
    "slug": "namaz-vakitleri-family",
    "scheme": "prayertime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notification_icon.png",
      "color": "#2563EB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.namazvakitleri.family",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to calculate accurate prayer times."
      }
    },
    "android": {
      "package": "com.namazvakitleri.family",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b1cba9c7-0da7-48c8-98ba-7dc4968a34f2"
      }
    }
  }
}
